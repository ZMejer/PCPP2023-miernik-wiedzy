QProgressBar {
    border-radius: 5px;
    background-color: #6C6482;
}

QProgressBar::chunk {
    border-radius: 5px;
    background-color: #6EA474;
}

QPushButton {
    border:none;
    background-color: #4b455e;
    color:white;
    font-weight:bold;
    font-size:28px;
    border-radius:5px;
}

QPushButton:hover {
    background-color:#565266;
}
